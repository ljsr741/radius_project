@import "../head.styl"
.setting_cells
  width 100%

  .setting_cell
    height 200px
    list-style none
    margin 0
    padding 0
    .setting_list
      height 50px
      background-color #fff
      margin-top 10px
      line-height 50px
      padding-left 10px
      position relative
      color #323232
      font-size 14px
      .setting_icon
        height 30px
        width 50px
        border-radius 25px
        background-color orangered
        position absolute
        bottom 10px
        right 10px
        .set_icon
          height 29px
          width 29px
          border-radius 50%
          background white
          position absolute
          bottom 10px
          right 21px
          top 0
          box-shadow 1px 1px 1px 1px #ccc
          z-index 5
      .setting_right:after
        content ''
        display block
        height 36px
        width 20px
        border-radius 25px
        background url("../../img/log in_right_icon@2x.png") no-repeat center
        background-size 50%
        position absolute
        bottom 7px
        right 10px
      .setting_left
        display block
        position absolute
        bottom 1px
        right 30px
        color #cecece
        font-size 14px
    .span
      text-align center

